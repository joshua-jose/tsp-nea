{
    "name": "tsp-nea",
    "version": "0.1.0",
    "description": "",
    "private": true,
    "main": "./.webpack/main",
    "scripts": {
        "build": "electron-forge make",
        "start": "electron-forge start",
        "dist": "electron-forge package"
    },
    "repository": "",
    "author": "",
    "license": "",
    "devDependencies": {
        "@electron-forge/cli": "6.0.0-beta.63",
        "@electron-forge/maker-zip": "6.0.0-beta.63",
        "@electron-forge/plugin-webpack": "6.0.0-beta.63",
        "@vercel/webpack-asset-relocator-loader": "1.7.0",
        "css-loader": "^6.5.1",
        "electron": "^16.0.2",
        "electron-reloader": "^1.2.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "webpack": "^5.65.0"
    },
    "dependencies": {
        "zeromq": "^6.0.0-beta.6"
    },
    "config": {
        "forge": {
            "makers": [
                {
                    "name": "@electron-forge/maker-zip"
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "preload": {
                            "js": "./app/main/preload.js"
                        },
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./app/renderer/index.html",
                                    "js": "./app/renderer/index.js",
                                    "name": "main_window"
                                }
                            ]
                        },
                        "devContentSecurityPolicy": "default-src 'self' 'unsafe-inline' data: https://*.jsdelivr.net https://*.jquery.com"
                    }
                ]
            ]
        }
    }
}